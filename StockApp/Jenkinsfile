pipeline {
agent any
stages {
stage('Clean stage') {
steps {
dir("StockApp") {
bat 'mvn clean'
}
}
}

stage('Compile stage') {
steps {
dir("StockApp") {
withMaven(maven: 'maven_3_6_1') {
bat 'mvn compile'
}
}
}
}

stage('Install stage') {
steps {
dir("StockApp") {
withMaven(maven: 'maven_3_6_1') {
bat 'mvn install'
}
}
}
}

}
}
